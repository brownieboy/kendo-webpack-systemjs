{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "extends": "eslint",
    "rules": {
        "semi": 2,
        "valid-jsdoc": [2, {
            "prefer": {
                "return": "return"
            }
        }]
    }
}