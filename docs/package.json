    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.7.7",
    "del": "^2.2.0",
    "esdoc": "^0.4.6",
    "gulp": "^3.9.1",
    "gulp-html-replace": "^1.5.5",
    "gulp-inject": "^4.0.0",
    "open": "0.0.5",
    "path": "^0.12.7",
    "run-sequence": "^1.1.5",
    "systemjs": "^0.19.24",
    "systemjs-plugin-babel": "0.0.8",
